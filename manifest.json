{
    "manifest_version": 3,
    "name": "test",
    "description": "a new test",
    "version": "1.0",
    "permissions": ["activeTab", "storage", "scripting"],
    "icons" : {
        "16": "images/capybara.png",
        "48": "images/capybara.png"
    },
    "action": {
        "default_title": "Click to view a popup",
        "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["hiddenInfo.js"]
      }
    ]
}
  
